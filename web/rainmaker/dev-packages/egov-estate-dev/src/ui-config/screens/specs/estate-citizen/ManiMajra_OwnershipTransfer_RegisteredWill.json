            "options": {
              "moduleName": "EstateServices",
              "masterName": "benificiaryRelationshipwithDeceased"
            }
          }
        ],
        "first_step": {
          "sections": [
            {
              "header": "ES_DECEASED_DETAILS_HEADER",
              "type": "FORM",
              "fields": [
                {
                  "label": "ES_DECEASED_LABEL",
                  "placeholder": "ES_DECEASED_NAME_PLACEHOLDER",
                  "type": "DROP_DOWN",
                  "jsonPath": "Applications[0].applicationDetails.transferor.id",
                  "path": "transferor.id",
                  "dataSource": "owners"
                },
                {
                  "label": "ES_DECEASED_NAME_LABEL",
                  "placeholder": "ES_DECEASED_NAME_PLACEHOLDER",
                  "disabled": true,
                  "required": true,
                  "type": "TEXT_FIELD",
                  "jsonPath": "Applications[0].applicationDetails.transferor.name",
                  "prefillValue": "selectedOwner.ownerDetails.ownerName",
                  "visible": false
                },
                {
                  "label": "ES_DECEASED_FATHER_HUSBAND_NAME_LABEL",
                  "placeholder": "ES_DECEASED_FATHER_HUSBAND_NAME_PLACEHOLDER",
                  "type": "TEXT_FIELD",
                  "disabled": true,
                  "prefillValue": "selectedOwner.ownerDetails.guardianName",
                  "jsonPath": "Applications[0].applicationDetails.transferor.fatherOrHusbandName"
                },
                {
                  "label": "ES_DECEASED_ADDRESS_LABEL",
                  "placeholder": "ES_DECEASED_ADDRESS_PLACEHOLDER",
                  "disabled": true,
                  "type": "TEXT_AREA",
                  "prefillValue": "selectedOwner.ownerDetails.address",
                  "jsonPath": "Applications[0].applicationDetails.transferor.address"
                },
                {
                  "label": "ES_PERCENT_SHARE_LABEL",
                  "placeholder": "ES_PERCENT_SHARE_PLACEHOLDER",
                  "type": "TEXT_FIELD",
                  "disabled": true,
                  "prefillValue": "selectedOwner.share",
                  "jsonPath": "Applications[0].applicationDetails.transferor.percentageOfShare"
                },
                {
                  "label": "ES_DATE_OF_DEATH_LABEL",
                  "type": "DATE_FIELD",
                  "path": "transferor.dateOfDeath",
                  "jsonPath": "Applications[0].applicationDetails.transferor.dateOfDeath"
                }
              ]
            },
            {
              "header": "ES_REGISTERED_WILL_DETAILS_HEADER",
              "type": "FORM",
              "fields": [
                {
                  "label": "ES_MODE_OF_TRANSFER_LABEL",
                  "placeholder": "ES_MODE_OF_TRANSFER_PLACEHOLDER",
                  "type": "DROP_DOWN",
                  "jsonPath": "Applications[0].applicationDetails.modeOfTransfer",
                  "path": "modeOfTransfer",
                  "dataSource": "modesOfTransfer"
                },
                {
                  "label": "ES_DATE_OF_EXECUTION_OF_WILL_LABEL",
                  "type": "DATE_FIELD",
                  "jsonPath": "Applications[0].applicationDetails.dateOfExecution",
                  "path": "dateOfExecution"
                },
                {
                  "label": "ES_REGISTRATION_NUMBER_OF_WILL_LABEL",
                  "placeholder": "ES_REGISTRATION_NUMBER_OF_WILL_PLACEHOLDER",
                  "type": "TEXT_FIELD",
                  "jsonPath": "Applications[0].applicationDetails.registrationNumber",
                  "path": "registrationNumber"
                },
                {
                  "label": "ES_DATE_OF_REGISTRATION_OF_WILL_LABEL",
                  "type": "DATE_FIELD",
                  "jsonPath": "Applications[0].applicationDetails.dateOfRegistration",
                  "path": "dateOfRegistration"
                }
              ]
            },
            {
              "header": "ES_LEGAL_BENEFICIARY_DETAILS",
              "type": "FORM",
              "fields": [
                {
                  "label": "ES_IS_LEGAL_HEIR_OR_BENEFICIARY_RADIO_BTN_LABEL",
                  "placeholder": "ES_IS_LEGAL_HEIR_OR_BENEFICIARY_RADIO_BTN_PLACEHOLDER",
                  "type": "RADIO_BUTTON",
                  "jsonPath": "Applications[0].applicationDetails.transferee.legalHeirOrbeneficiary",
                  "path": "transferee.legalHeirOrbeneficiary"
                },
                {
                  "label": "ES_IS_LEGAL_HEIR_OR_BENEFICIARY_EXISTING_OWNER_RADIO_BTN_LABEL",
                  "placeholder": "ES_IS_LEGAL_HEIR_OR_BENEFICIARY_EXISTING_OWNER_RADIO_BTN_PLACEHOLDER",
                  "type": "DROP_DOWN",
                  "jsonPath": "Applications[0].applicationDetails.transferee.type",
                  "path": "transferee.type",
                  "dataSource": "typesOfOwner",
                  "visibility": "owners.length > 1 && purchasers.length > 0"
                },
                {
                  "label": "ES_SELECT_OWNER_LABEL",
                  "placeholder": "ES_SELECT_OWNER_PLACEHOLDER",
                  "type": "DROP_DOWN",
                  "jsonPath": "Applications[0].applicationDetails.transferee.id",
                  "path": "transferee.id",
                  "dataSource": "owners",
                  "dataValue": "purchasers",
                  "prefillValue": "selectedPurchaser ? selectedPurchaser.id : ''",
                  "visible": false,
                  "required": true,
                  "visibility": "application.applicationDetails.transferee.type == 'Existing'"
                },
                {
                  "label": "ES_LEGAL_HEIR_BENEFICIARY_NAME_LABEL",
                  "placeholder": "ES_LEGAL_HEIR_BENEFICIARY_NAME_PLACEHOLDER",
                  "type": "TEXT_FIELD",
                  "jsonPath": "Applications[0].applicationDetails.transferee.name",
                  "path": "transferee.name",
                  "visibility": "application.applicationDetails.transferee.type !== 'Existing'",
                  "errorMessage": "ES_ERR_LEGAL_BENEFICIARY_NAME",
                  "prefillValue": "application.applicationDetails.transferee.type == 'Existing' && selectedPurchaser ? selectedPurchaser.ownerDetails.ownerName : application.applicationDetails.transferee.name "
                },
                {
                  "label": "ES_LEGAL_BENIFICIARY_FATHER_HUSBAND_NAME_LABEL",
                  "placeholder": "ES_FATHER_HUSBAND_NAME_PLACEHOLDER",
                  "type": "TEXT_FIELD",
                  "jsonPath": "Applications[0].applicationDetails.transferee.fatherOrHusbandName",
                  "path": "transferee.fatherOrHusbandName",
                  "disability": "application.applicationDetails.transferee.type == 'Existing'",
                  "prefillValue": "application.applicationDetails.transferee.type == 'Existing' ? selectedPurchaser.ownerDetails.guardianName : application.applicationDetails.transferee.fatherOrHusbandName",
                  "errorMessage": "ES_ERR_FATHER_HUSBAND_NAME"
                },
                {
                  "label": "ES_RELATION_SHIP_LABEL",
                  "type": "RADIO_BUTTON",
                  "jsonPath": "Applications[0].applicationDetails.transferee.relation",
                  "path": "transferee.relation",
                  "disability": "application.applicationDetails.transferee.type == 'Existing'",
                  "prefillValue": "application.applicationDetails.transferee.type == 'Existing' ? selectedPurchaser.ownerDetails.guardianRelation : application.applicationDetails.transferee.relation"
                },
                {
                  "label": "ES_DOB_LABEL",
                  "placeholder": "ES_DOB_PLACEHOLDER",
                  "type": "DATE_FIELD",
                  "jsonPath": "Applications[0].applicationDetails.transferee.dob",
                  "path": "transferee.dob",
                  "disability": "application.applicationDetails.transferee.type == 'Existing'",
                  "prefillValue": "application.applicationDetails.transferee.type == 'Existing' && selectedPurchaser ? selectedPurchaser.ownerDetails.dob : application.applicationDetails.transferee.dob "
                },
                {
                  "label": "ES_ADDRESS_LABEL",
                  "placeholder": "ES_ADDRESS_PLACEHOLDER",
                  "type": "TEXT_AREA",
                  "jsonPath": "Applications[0].applicationDetails.transferee.address",
                  "path": "transferee.address",
                  "disability": "application.applicationDetails.transferee.type == 'Existing'",
                  "prefillValue": "application.applicationDetails.transferee.type == 'Existing' ? selectedPurchaser.ownerDetails.address : application.applicationDetails.transferee.address",
                  "errorMessage": "ES_ERR_ADDRESS"
                },
                {
                  "label": "ES_BENEFICIARY_RELATION_WITH_DECEASED_RADIO_BTN_LABEL",
                  "placeholder": "ES_BENEFICIARY_RELATION_WITH_DECEASED_RADIO_BTN_PLACEHOLDER",
                  "type": "DROP_DOWN",
                  "jsonPath": "Applications[0].applicationDetails.transferee.relationWithDeceased",
                  "dataSource": "benificiaryRelationshipwithDeceased",
                  "visibility": "application.applicationDetails.transferee.legalHeirOrbeneficiary === 'BENEFICIARY'"
                },
                {
                  "label": "ES_PERCENTAGE_SHARED_LABEL",
                  "placeholder": "ES_PERCENTAGE_SHARED_PLACEHOLDER",
                  "type": "TEXT_FIELD",
                  "jsonPath": "Applications[0].applicationDetails.transferee.percentageOfShareTransferred",
                  "path": "transferee.percentageOfShareTransferred",
                  "pattern": "Percentage",
                  "errorMessage": "ES_ERR_PERCENTAGE"
                },
                {
                  "label": "ES_REMAINING_SHARE_PERCENTAGE",
                  "type": "TEXT_FIELD",
                  "disabled": true,
                  "prefillValue": "!!application.applicationDetails.transferee.percentageOfShareTransferred && !!selectedOwner.share ? selectedOwner.share - (selectedOwner.share * application.applicationDetails.transferee.percentageOfShareTransferred / 100) : ''"
                }
              ]
            },
            {
              "header": "ES_WITNESS_DETAILS_HEADER",
              "type": "FORM",
              "fields": [
                {
                  "label": "ES_WITNESS_NAME_LABEL",
                  "placeholder": "ES_WITNESS_NAME_PLACEHOLDER",
                  "type": "TEXT_FIELD",
                  "jsonPath": "Applications[0].applicationDetails.witness[0].name",
                  "path": "witness.name",
                  "errorMessage": "ES_ERR_WITNESS_NAME"
                },
                {
                  "label": "ES_FATHER_HUSBAND_NAME_LABEL",
                  "placeholder": "ES_FATHER_HUSBAND_NAME_PLACEHOLDER",
                  "type": "TEXT_FIELD",
                  "jsonPath": "Applications[0].applicationDetails.witness[0].fatherOrHusbandName",
                  "path": "witness.name",
                  "errorMessage": "ES_ERR_WITNESS_NAME"
                },
                {
                  "label": "ES_DOB_LABEL",
                  "placeholder": "ES_DOB_PLACEHOLDER",
                  "type": "DATE_FIELD",
                  "jsonPath": "Applications[0].applicationDetails.witness[0].dob",
                  "path": "witness.dob"
                },
                {
                  "label": "ES_ADDRESS_LABEL",
                  "placeholder": "ES_ADDRESS_PLACEHOLDER",
                  "type": "TEXT_AREA",
                  "jsonPath": "Applications[0].applicationDetails.witness[0].address",
                  "path": "witness.address",
                  "errorMessage": "ES_ERR_ADDRESS"
                },
                {
                  "label": "ES_MOBILE_NUMBER_LABEL",
                  "placeholder": "ES_MOBILE_NUMBER_PLACEHOLDER",
                  "type": "TEXT_FIELD",
                  "jsonPath": "Applications[0].applicationDetails.witness[0].mobileNo",
                  "path": "witness.mobileNo",
                  "pattern": "MobileNo",
                  "errorMessage": "ES_ERR_MOBILE_NUMBER"
                }
              ]
            }
          ]
        },
