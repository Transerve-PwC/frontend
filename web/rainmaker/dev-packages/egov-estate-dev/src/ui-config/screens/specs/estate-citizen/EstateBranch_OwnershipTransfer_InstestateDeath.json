{
    "tenantId": "ch",
    "moduleName": "EstatePropertyService",
    "EstateBranch_OwnershipTransfer_InstestateDeath": {
      "dataSources" : [
        {
            "name": "typesOfOwner",
            "type": "local",
            "cacheable" : true,
            "lazyload" : true,
            "values": [{"code": "New"}, {"code": "Existing"}]
        },
        {
            "name": "modesOfTransfer",
            "type": "mdms",
            "cacheable" : true,
            "lazyload" : true,
            "options" : {
              "moduleName" : "EstatePropertyService",
              "masterName" : "modeOfTransfer"
            }
        },
        {
          "name": "applicationDocuments",
          "type": "mdms",
          "lazyload" : true,
          "cacheable" : true,
          "options" : {
            "moduleName" : "EstateProperties",
            "masterName" : "applicationDocuments",
            "filter" : "[?(@.code=='MasterRP')]"
          }
      }
      ],
      "fields": [
        {
          "path": "modeOfTransfer",
          "required": true,
          "validations": [
            {
              "type": "mdms",
              "params": {
                "moduleName": "EstatePropertyService",
                "masterName": "modeOfTransfer",
                "filter": "modeOfTransfer..code"
              }
            }
          ]
        },
        {
          "path": "registrationNumber",
          "required": false,
          "validations": [
            {
              "type": "length",
              "params": {
                "min": 0,
                "max": 100
              }
            }
          ]
        },
        {
          "path": "dateOfRegistration",
          "required": false,
          "validations": [
            {
              "type": "date-range",
              "params": {
                "start": {
                  "unit": "year",
                  "value": -40
                },
                "end": {
                  "unit": "second",
                  "value": 0
                }
              }
            }
          ]
        },
        {
          "path": "owner",
          "required": true,
          "validations": [
            {
              "type": "object"
            }
          ]
        },
        {
          "path": "owner.id",
          "required": true,
          "validations": [
            {
              "type": "owner"
            }
          ]
        },
        {
          "path": "purchaser",
          "required": true,
          "validations": [
            {
              "type": "object"
            }
          ]
        },
        {
          "path": "purchaser.name",
          "required": true,
          "validations": [
            {
              "type": "length",
              "params": {
                "min": 5,
                "max": 150
              }
            }
          ]
        },
        {
          "path": "purchaser.fatherOrHusbandName",
          "required": true,
          "validations": [
            {
              "type": "length",
              "params": {
                "min": 5,
                "max": 150
              }
            }
          ]
        },
        {
          "path": "purchaser.relation",
          "required": true,
          "validations": [
            {
              "type": "enum",
              "params": {
                "values": [
                  "FATHER",
                  "HUSBAND"
                ]
              }
            }
          ]
        },
        {
          "path": "purchaser.address",
          "required": true,
          "validations": [
            {
              "type": "length",
              "params": {
                "min": 5,
                "max": 150
              }
            }
          ]
        },
        {
          "path": "purchaser.mobileNo",
          "required": false,
          "validations": [
            {
              "type": "phonenumber"
            }
          ]
        },
        {
          "path": "purchaser.percentageOfShareTransferred",
          "required": true,
          "validations": [
            {
              "type": "numeric"
            },
            {
              "type": "minmax",
              "params": {
                "min": 0,
                "max": 100
              }
            }
          ]
        }
      ],
      "first_step": {
        "sections": [
          {
            "header": "EST_OWNERS_DETAILS_HEADER",
            "type": "CARD_DETAIL",
            "fields": [
              {
                "label": "EST_OWNER_NAME_LABEL",
                "jsonPath": "property.fileNumber"
              },
              {
                "label": "EST_OWNER_ADDRESS_LABEL",
                "jsonPath": "property.category"
              },
              {
                "label": "EST_SHARES_LABEL",
                "jsonPath": "property.subCategory"
              }
            ]
          },
          {
            "header": "EST_DECEASED_DETAILS_HEADER",
            "type": "FORM",
            "fields": [
              {
                "label": "EST_DECEASED_NAME_TYPE_LABEL",
                "placeholder": "EST_DECEASED_NAME_TYPE_PLACEHOLDER",
                "type": "DROP_DOWN",
                "jsonPath": "payload.applicationDetails.purchaser.type",
                "path": "purchaser.type",
                "dataSource": "typesOfOwner"
              },
              {
                "label": "EST_DECEASED_FATHER_HUSBAND_NAME_LABEL",
                "placeholder": "EST_DECEASED_FATHER_HUSBAND_NAME_PLACEHOLDER",
                "type": "TEXT_FIELD",
                "jsonPath": "payload.applicationDetails.purchaser.name",
                "path": "purchaser.name"
              },
              {
                "label": "EST_DECEASED_ADDRESS_LABEL",
                "placeholder": "EST_DECEASED_ADDRESS_PLACEHOLDER",
                "type": "TEXT_FIELD",
                "jsonPath": "payload.applicationDetails.purchaser.fatherOrHusbandName",
                "path": "purchaser.fatherOrHusbandName",
                "disabled": "!!state.screenConfiguration.preparedFinalObject.payload && !!state.screenConfiguration.preparedFinalObject.payload.purchaser && !!state.screenConfiguration.preparedFinalObject.payload.purchaser.name"
              },
              {
                "label": "EST_PERCENT_SHARE_LABEL",
                "placeholder": "EST_PERCENT_SHARE_PLACEHOLDER",
                "type": "TEXT_FIELD",
                "jsonPath": "payload.applicationDetails.purchaser.relation",
                "path": "purchaser.relation"
              },
              {
                "label": "EST_DATE_OF_DEATH_LABEL",
                "placeholder": "EST_DATE_OF_DEATH_PLACEHOLDER",
                "type": "DATE_FIELD",
                "jsonPath": "payload.applicationDetails.purchaser.address",
                "path": "purchaser.address"
              },
              {
                "label": "EST_IS_LEGAL_HEIR_LABEL",
                "placeholder": "EST_IS_LEGAL_HEIR_PLACEHOLDER",
                "type": "RADIO_BUTTON",
                "jsonPath": "payload.applicationDetails.purchaser.relation",
                "path": "purchaser.relation"
              },
              {
                "label": "EST_IS_EXISTING_OWNER_LABEL",
                "placeholder": "EST_IS_EXISTING_OWNER_PLACEHOLDER",
                "type": "RADIO_BUTTON",
                "jsonPath": "payload.owner.id",
                "path": "purchaser.relation"
              },
              {
                "label": "EST_SELECT_OWNER_LABEL",
                "placeholder": "EST_SELECT_OWNER_PLACEHOLDER",
                "type": "DROP_DOWN",
                "jsonPath": "payload.applicationDetails.purchaser.percentageOfShareTransferred",
                "path": "purchaser.percentageOfShareTransferred",
                "pattern": "Amount"
              }
             
            ]
          },
          {
            "header": "EST_LEGAL_HEIR_DETAILS_HEADER",
            "type": "FORM",
            "fields": [
              {
                "label": "EST_LEGAL_HEIR_NAME_LABEL",
                "placeholder": "EST_LEGAL_HEIR_PLACEHOLDER",
                "type": "TEXT_FIELD",
                "jsonPath": "payload.applicationDetails.purchaser.type",
                "path": "purchaser.type",
                "dataSource": "typesOfOwner"
              },
              {
                "label": "EST_LEGAL_HEIR_AGE_LABEL",
                "placeholder": "EST_LEGAL_HEIR_AGE_PLACEHOLDER",
                "type": "TEXT_FIELD",
                "jsonPath": "payload.applicationDetails.purchaser.name",
                "path": "purchaser.name"
              },
              {
                "label": "EST_LEGAL_HEIR_ADDRESS_LABEL",
                "placeholder": "EST_LEGAL_HEIR_ADDRESS_PLACEHOLDER",
                "type": "TEXT_FIELD",
                "jsonPath": "payload.applicationDetails.purchaser.fatherOrHusbandName",
                "path": "purchaser.fatherOrHusbandName",
                "disabled": "!!state.screenConfiguration.preparedFinalObject.payload && !!state.screenConfiguration.preparedFinalObject.payload.purchaser && !!state.screenConfiguration.preparedFinalObject.payload.purchaser.name"
              },
              {
                "label": "EST_LEGAL_HEIR_RELATIONSHIP_WITH_DECEASED_LABEL",
                "placeholder": "EST_LEGAL_HEIR_RELATIONSHIP_WITH_DECEASED_PLACEHOLDER",
                "type": "RADIO_BUTTON",
                "jsonPath": "payload.applicationDetails.purchaser.relation",
                "path": "purchaser.relation"
              }
            ]
          }
        ]
      },
      "second_step": {
        "header": "EST_DOCUMENT_DETAILS_HEADER",
        "jsonPath": "payload.documents",
        "documentList": [
          {
            "code": "CERTIFIED_COPY_OF_LEASE_RIGHT",
            "required": true,
            "accept": "application/msword,application/pdf,application/vnd.openxmlformats-officedocument.wordprocessingml.document,image/*",
            "fileType": "ALLTYPES",
            "description": "CERTIFIED_COPY_OF_LEASE_RIGHT_DESCRIPTION"
          },
          {
            "code": "NOTARIZED_COPY_OF_SALE_DEED",
            "required": true,
            "accept": "application/msword,application/pdf,application/vnd.openxmlformats-officedocument.wordprocessingml.document,image/*",
            "fileType": "ALLTYPES",
            "description": "NOTARISED_COPY_OF_SALE_DEED_DESCRIPTION"
          },
          {
            "code": "SELF_ATTESTED_PHOTO",
            "required": true,
            "accept": "application/msword,application/pdf,application/vnd.openxmlformats-officedocument.wordprocessingml.document,image/*",
            "fileType": "ALLTYPES",
            "description": "SELF_ATTESTED_PHOTO_TRANSFEREE_DESCRIPTION"
          },
          {
            "code": "IDEMINITY_BOND",
            "required": true,
            "accept": "application/msword,application/pdf,application/vnd.openxmlformats-officedocument.wordprocessingml.document,image/*",
            "fileType": "ALLTYPES",
            "description": "IDEMINITY_BOND_OF_TRANSFEREE_DESCRIPTION"
          },
          {
            "code": "SELF_ATTESTED_PHOTO_WITNESSES",
            "required": true,
            "accept": "application/msword,application/pdf,application/vnd.openxmlformats-officedocument.wordprocessingml.document,image/*",
            "fileType": "ALLTYPES",
            "description": "SELF_ATTESTED_PHOTO_WITNESSES_DESCRIPTION"
          },
          {
            "code": "CLEARANCE_OF_PREVIOUS_LOANS",
            "required": true,
            "accept": "application/msword,application/pdf,application/vnd.openxmlformats-officedocument.wordprocessingml.document,image/*",
            "fileType": "ALLTYPES",
            "description": "CLEARANCE_OF_PREVIOUS_DESCRIPTION"
          },
          {
            "code": "SEWERAGE_CONNECTION_OCCUPATION_CERTIFICATE_FURNISH_PROOF",
            "required": true,
            "accept": "application/msword,application/pdf,application/vnd.openxmlformats-officedocument.wordprocessingml.document,image/*",
            "fileType": "ALLTYPES",
            "description": "SEWERAGE_CONNECTION_OCCUPATION_CERTIFICATE_FURNISH_PROOF_DESCRIPTION"
          },
          {
            "code": "AN_UNDERTAKING_BY_WAY_INDEMITY_BOND",
            "required": true,
            "accept": "application/msword,application/pdf,application/vnd.openxmlformats-officedocument.wordprocessingml.document,image/*",
            "fileType": "ALLTYPES",
            "description": "AN_UNDERTAKING_BY_WAY_INDEMITY_BOND_DESCRIPTION"
          },
          {
            "code": "NOTARIZED_COPY_GPA_SPA",
            "required": true,
            "accept": "application/msword,application/pdf,application/vnd.openxmlformats-officedocument.wordprocessingml.document,image/*",
            "fileType": "ALLTYPES",
            "description": "NOTARIZED_COPY_GPA_SPA_DESCRIPTION"
          },
          {
            "code": "AFFIDAVIT_REGARDING_GPA_SPA",
            "required": true,
            "accept": "application/msword,application/pdf,application/vnd.openxmlformats-officedocument.wordprocessingml.document,image/*",
            "fileType": "ALLTYPES",
            "description": "AFFIDAVIT_REGARDING_GPA_SPA_DESCRIPTION"
          },
          {
            "code": "AFFIDAVIT_EFFECT_UNDER_HAND_SALE",
            "required": true,
            "accept": "application/msword,application/pdf,application/vnd.openxmlformats-officedocument.wordprocessingml.document,image/*",
            "fileType": "ALLTYPES",
            "description": "AFFIDAVIT_EFFECT_UNDER_HAND_SALE_DESCRIPTION"
          },
          {
            "code": "NO_DUE_CERTIFICATE",
            "required": true,
            "accept": "application/msword,application/pdf,application/vnd.openxmlformats-officedocument.wordprocessingml.document,image/*",
            "fileType": "ALLTYPES",
            "description": "NO_DUE_CERTIFICATE_DESCRIPTION"
          }
        ]
      }
    }
  }